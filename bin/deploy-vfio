#!/bin/bash/env bash

#
# Filename:       deploy-vfio
# Description:    Main executable. See README for details and usage.
# Author(s):      Alex Portell <github.com/portellam>
# Maintainer(s):  Alex Portell <github.com/portellam>
#

# <sets>
  set -o pipefail
# </sets>

# <sources>
  source deploy-vfio_sources
# </sources>

# <functions>
  function main
  {
    is_user_superuser
    get_options "$@"
    is_system_supported
    do_pre_setup || exit 1
    do_parse_database || exit 1
    do_main_setup || exit 1
    do_post_setup || exit 1
  }
# </functions>

# <code>
  main "$@"
# </code>