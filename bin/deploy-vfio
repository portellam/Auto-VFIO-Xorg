# <functions>
  function Main
  {
    IsUserSuperuser
    SetArguments "$@"
    IsSystemSupported
    DoPreSetup || exit 1
    DoMainSetup || exit 1
    DoPostSetup || exit 1
  }
# </functions>

# <code>
  Main "$@"
# </code>