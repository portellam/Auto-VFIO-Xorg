# <functions>
  function Main
  {
    IsUserSuperuser || exit 1
    SetArguments "$@" || exit 1
    IsSystemSupported || exit 1
    DoPreSetup
    DoMainSetup
    DoPostSetup
  }
# </functions>

# <code>
  Main "$@"
# </code>